<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script>
        function signin(){
            var form = document.getElementById("form_info");

            axios({
                method:"post", 
                url:"/axiospost", 
                data:{      
                    id:form.id.value,
                    pw:form.pw.value


                }
            }). then(function(res){ 
                        console.log(res)    
                        // res에는 data,request등 여러가지의 status가 나타나고 그중에서 data만 뽑기위해 data를 샐랙 해줘야 함
                        document.querySelector("#result").innerHTML =res.data.msg;
                    
                }

                   
            )}
        

            function signinget(){
            var form = document.getElementById("form_info");

            var b = axios({
                method:"get", 
                url:"/axiosget", 
                params:{      //서버로 넘길 부분이 params여야 함
                    id:form.id.value,
                    pw:form.pw.value


                }
                
            }) 
            . then(function(res){ 
                          console.log(res)
                        // res에는 data,request등 여러가지의 status가 나타나고 그중에서 data만 뽑기위해 data를 샐랙 해줘야 함
                        document.querySelector("#result").innerHTML =res.data.msg;
                    
                }

                   
            )
            console.log(b)}
        

    </script>
</head>
<body>
    <form id ="form_info" >
        아이디<input type="text" name="id" value="">
        비밀번호<input type="password" name="pw" value="">

        
    </form>

    <button type="button" onclick = "signin()">post로그인</button>
    <button type="button" onclick = "signinget()">get로그인</button>
    <div id=result></div>
</body>
</html>
